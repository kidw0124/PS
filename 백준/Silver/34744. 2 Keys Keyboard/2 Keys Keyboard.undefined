#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    vector<int>arr(n+1,-1);
    function<int(int)>f=[&f,&arr](int x){
        if(x==1)return 0;
        if(arr[x]!=-1)return arr[x];
        int ret=1e9;
        for(int i=1;i*i<=x;i++){
            if(x%i==0){
                ret=min({ret,f(i)+x/i,i==1?ret:f(x/i)+i});
            }
        }
        return arr[x]=ret;
    };
    cout<<f(n);
}