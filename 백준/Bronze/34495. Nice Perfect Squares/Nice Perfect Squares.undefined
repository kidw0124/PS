#include<bits/stdc++.h>
using namespace std;
int main(){
    int t,n;
    cin>>t;
    while(t--){
        cin>>n;
        if(n&1){
            cout<<"42025";
            while(int m=n-5){
                cout<<"0";n--;
            }
            cout<<"\n";
        }
        else{
            cout<<"2025";
            while(int m=n-4){
                cout<<"0";n--;
            }
            cout<<"\n";
        }
    }
}